{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "chrome": "41",
          "ie": "11",
          "edge": "16"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": ["lodash", "@babel/syntax-dynamic-import", "@babel/plugin-proposal-class-properties"],
  "sourceType": "unambiguous"
}
